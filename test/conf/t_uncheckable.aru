-- We make a "clear all" before and after the test to avoid interferences when run
-- from the stress test
go;
clear all;
Uncheckable: check uncheckable;

check allocators (inconsistent);
check case_statement (others_span, min 5);
check case_statement (values, min 5);
check case_statement (range_span, min 3);
check declarations (binary_modular_type);
check directly_accessed_globals;
check duplicate_initialization_calls (all out_proc);
check exception_propagation (0, parameter, t_uncheckable.exception_propagation.make_cb_l0.cb);
check generic_aliasing;
check global_references (all, t_uncheckable.global_references);
check max_call_depth (finite);
check object_declarations (volatile_no_address);
check object_declarations (address_not_volatile);
check parameter_aliasing;
check potentially_blocking_operations;
check record_declarations (component, array, not aligned);
check record_declarations (component, range,     aligned);
check representation_clauses (fractional_size, non_contiguous_layout);
check style (renamed_entity);
check unsafe_paired_calls (all P, all V);
check unsafe_unchecked_conversion;
check usage (object);

