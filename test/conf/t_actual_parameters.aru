-- Subrule Default
       search  actual_parameters (default, t_actual_parameters.default.gen, x,   used);
       search  actual_parameters (default, t_actual_parameters.default.gen, y,   used);
All_1: check   actual_parameters (default, t_actual_parameters.default.gen, all, not_used);

       search  actual_parameters (default, t_actual_parameters.default.proc, x,   used);
       search  actual_parameters (default, t_actual_parameters.default.proc, y,   used);
All_C: search  actual_parameters (default, calls,                            all, not_used);
All_I: search  actual_parameters (default, instantiations,                   all, not_used);

       check actual_parameters (default, calls,          "<", not_used);
       check actual_parameters (default, instantiations, "<", not_used);
Pos:   check actual_parameters (default, calls,          all, positional);
Pos:   check actual_parameters (default, instantiations, all, positional);

-- Subrule Entity
"P/X/A":               check actual_parameters (entity, T_Actual_Parameters.Entity.P,   X,   T_Actual_Parameters.Entity.A);
"P/X/B":               check actual_parameters (entity, T_Actual_Parameters.Entity.P,   X,   T_Actual_Parameters.Entity.B);
"all/Y/B":             check actual_parameters (entity, calls,                          Y,   T_Actual_Parameters.Entity.B);
"Calls/X/F_No_Param":  check actual_parameters (entity, calls,                          X,   T_Actual_Parameters{}.Entity{}.F{return Standard.Integer});
"Gen/V/B":             check actual_parameters (entity, T_Actual_Parameters.Entity.gen, v,   T_Actual_Parameters{}.Entity.B);
"Gen/F/F_No_Param":    check actual_parameters (entity, T_Actual_Parameters.Entity.gen, f,   T_Actual_Parameters{}.Entity{}.F{return Standard.Integer});
"Instance":            check actual_parameters (entity, calls,                          all, Ada.Unchecked_Conversion);

